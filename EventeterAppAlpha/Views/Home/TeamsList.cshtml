@model IEnumerable<EventeterAppAlpha.Models.GetTeamList_Result>
@{
    ViewBag.Title = "TeamsList";
}
<button type="button" class="btn btn-primary" value="Home" onclick="location.href='@Url.Action("Index", "Home")'">
    Home
</button>
<h2>TeamsList</h2>


<div class="panel panel-default">
    <div class="panel-heading"> Here's a list of teams</div>
    <div class="panel-body">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.TeamName)

                </th>
              </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item.TeamName)
                    </td>
                    
                </tr>

            }
        </table>
    </div>
</div>
