@model IEnumerable<EventeterAppAlpha.Models.GetLeftOuterJoinUser_Team_Result>
@{
    ViewBag.Title = "UserTeam";
}
<button type="button" class="btn btn-primary" value="Home" onclick="location.href='@Url.Action("Index", "Home")'">
    Home
</button>
<h2>Users by Team</h2>

<div class="panel panel-default">
    <div class="panel-heading"> Here's a list of teams, and the users that belong to them</div>
    <div class="panel-body">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.Username)

                </th>
                <th>
                    @Html.DisplayNameFor(model => model.TeamName)

                </th>

            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item.Username)
                    </td>
                    <td>
                        @Html.DisplayFor(model => item.TeamName)
                    </td>

                </tr>

            }
        </table>
    </div>
</div>