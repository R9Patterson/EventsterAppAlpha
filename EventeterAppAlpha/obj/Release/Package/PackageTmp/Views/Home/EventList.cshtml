@model IEnumerable<EventeterAppAlpha.Models.GetEventsList_Result>
@{
    ViewBag.Title = "EventList";
}
<button type="button" class="btn btn-primary" value="Home" onclick="location.href='@Url.Action("Index", "Home")'">
    Home
</button>
<h2>EventList</h2>

<div class="panel panel-default">
    <div class="panel-heading"> Here's a list of events </div>
    <div class="panel-body">
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.EventTitle)

                </th>
               
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(model => item.EventTitle)
                    </td>
                  
                </tr>

            }
        </table>
    </div>
</div>
