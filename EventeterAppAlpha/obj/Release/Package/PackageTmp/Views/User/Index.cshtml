@model IEnumerable<EventeterAppAlpha.Models.User>

@{
    ViewBag.Title = "Index";
}

<h2>Store Procedure</h2>
<p>
    Returns a list of users from the user table
    in ascending order, by their user name.
</p>

<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.First().Username)
        </th>
    </tr>
    
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(model => item.Username)
            </td>
        </tr>
    }
</table>